{
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "go",
    "python",
    "json",
    "html"
  ],
  "languageserver": {
    "go": {
      "command": "${env:HOME}/go/bin/gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "trace.server": "verbose",
      "filetypes": ["go"],
      "initializationOptions": {
        "gofumpt": true
      }
    }
  },
  "python.venvPath": "${env:CWD}/../.env"
}
